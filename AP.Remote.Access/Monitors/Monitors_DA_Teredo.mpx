      <KnowledgeArticles>

        <!-- AP.Remote.Access.Monitor.DA_TEREDOSRV_HEURISTIC_FORWARDING -->
        <KnowledgeArticle ElementID="AP.Remote.Access.Monitor.DA_TEREDOSRV_HEURISTIC_FORWARDING" Visible="true">
          <MamlContent>
            <maml:section xmlns:maml="http://schemas.microsoft.com/maml/2004/10">
              <maml:title>Summary</maml:title>
              <maml:para>Forwarding is disabled on the Teredo server.</maml:para>
            </maml:section>
            <maml:section xmlns:maml="http://schemas.microsoft.com/maml/2004/10">
              <maml:title>Causes</maml:title>
              <maml:para>Forwarding is disabled on the Teredo server.</maml:para>
            </maml:section>
            <maml:section xmlns:maml="http://schemas.microsoft.com/maml/2004/10">
              <maml:title>Resolutions</maml:title>
              <maml:para>Enable forwarding on the Teredo server.</maml:para>
            </maml:section>
          </MamlContent>
        </KnowledgeArticle>

        <!-- AP.Remote.Access.Monitor.DA_TEREDOSRV_HEURISTIC_INIT_FAILED -->
        <KnowledgeArticle ElementID="AP.Remote.Access.Monitor.DA_TEREDOSRV_HEURISTIC_INIT_FAILED" Visible="true">
          <MamlContent>
            <maml:section xmlns:maml="http://schemas.microsoft.com/maml/2004/10">
              <maml:title>Summary</maml:title>
              <maml:para>The Teredo server cannot be started.</maml:para>
            </maml:section>
            <maml:section xmlns:maml="http://schemas.microsoft.com/maml/2004/10">
              <maml:title>Causes</maml:title>
              <maml:para>Initialization of the Teredo server has failed.</maml:para>
            </maml:section>
            <maml:section xmlns:maml="http://schemas.microsoft.com/maml/2004/10">
              <maml:title>Resolutions</maml:title>
              <maml:para>1.  Start the IP Helper service by typing 'restart-service iphlpsvc' from a Windows PowerShell prompt.</maml:para>
              <maml:para>2.  Determine the Teredo state.</maml:para>
            </maml:section>
          </MamlContent>
        </KnowledgeArticle>

